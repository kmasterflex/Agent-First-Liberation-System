{
  "name": "abc-terminal",
  "version": "1.0.0",
  "description": "ABC Terminal - Agent-Based Community Terminal",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "start:ui": "./claude-flow start --ui",
    "start:cli": "./claude-flow start",
    "start:docker": "docker-compose up -d",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "build:docker": "docker build -t claude-flow .",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "preinstall": "node -v | grep -qE \"v(1[89]|[2-9][0-9])\" || (echo \"Node.js 18+ required\" && exit 1)",
    "postinstall": "[ -f .env ] || cp .env.example .env",
    "setup": "npm install && npm run build && chmod +x claude-flow start.sh",
    "quick-start": "./start.sh",
    "sparc": "./claude-flow sparc",
    "swarm": "./claude-flow swarm",
    "monitor": "./claude-flow monitor",
    "logs": "tail -f logs/claude-flow.log",
    "health": "./claude-flow status",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:build": "docker-compose build",
    "docker:clean": "docker-compose down -v",
    "release": "npm run clean && npm run build && npm run test"
  },
  "keywords": [
    "terminal",
    "agents",
    "community",
    "llm"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.0",
    "@supabase/supabase-js": "^2.39.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "dotenv": "^16.4.0",
    "figlet": "^1.7.0",
    "gradient-string": "^2.0.2",
    "inquirer": "^9.2.15",
    "ora": "^8.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/figlet": "^1.7.0",
    "@types/gradient-string": "^1.1.6",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
